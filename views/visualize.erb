<% js_custom_add_file('javascripts/modelTableTooltips') %>

<% @models_to_column_lines.each do |model, column_lines| %>
  <div hidden id=<%="#{model}"%>>
    <h4>Schema</h4>
    <% column_lines.each do |line| %>
      <p><%=line%></p>
    <% end %>
  </div>
<% end %>

<div class="model-graph"> <%= inline_svg("graph.svg") %> </div>

<nav> 
  <a href="/">Visualize another project</a>
</nav>
